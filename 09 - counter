#include "stdafx.h"

#include <iostream>
using namespace std;


int main()
{
	int n1 = 0, n2 = 0;

	cout << "Enter a number to start counting: ";
	cin >> n1;
	cout << "Enter a number to end counting: ";
	cin >> n2;

	cout << endl;

	while (n1 <= n2) {
		cout << n1 << " ";
		n1++;
	}

	cout << endl << endl;
    return 0;
}
