// Convert.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"

#include <iostream>
using namespace std;


int main()
{
	double ctemp;

	cout << "Input a Celsius temp and press ENTER: ";
	cin >> ctemp;

	cout << "Fahrenheit temp is : " << (ctemp * 1.8) + 32 << endl;
    return 0;
}
